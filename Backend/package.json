{
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/sinon-chai": "^3.2.3",
    "axios": "^1.6.2",
    "chai": "^4.2.0",
    "ethereum-waffle": "^4.0.10",
    "ethers": "5.7",
    "hardhat": "^2.19.2",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.45",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.0",
    "typechain": "^8.3.0"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "4.9",
    "fhevm": "^0.2.2",
    "fhevmjs": "^0.2.2"
  },
  "scripts": {
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json ./deployments/localfhenix && pnpm typechain",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "deploy:contracts": "hardhat deploy",
    "lint": "pnpm lint:sol && pnpm lint:ts && pnpm prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "DOTENV_CONFIG_PATH=./.env.example pnpm typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "hardhat": "hardhat",
    "task:wrap": "hardhat task:wrap",
    "task:addCount": "hardhat task:addCount",
    "task:getCount": "hardhat task:getCount",
    "test": "hardhat test --network localfhenix",
    "ci-test": "hardhat test --network ci_localfhenix",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "start:localfhenix": "docker run -it -p 8545:8545 -p 6000:6000 -p 5000:5000 --rm --name localfhenix ghcr.io/fhenixprotocol/fhenix-devnet:0.1.6",
    "stop:localfhenix": "docker stop localfhenix && docker rm localfhenix",
    "restart:localfhenix": "stop:localfhenix && start:localfhenix",
    "faucet": "node getFromFaucet.js"
  },
  "name": "project",
  "version": "1.0.0",
  "license": "MIT"
}
